datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model account {
  id        String   @id
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  email     String   @unique
}

model question {
  id             String   @id @default(uuid())
  questionNumber Int      @unique
  urlSolution    String
  solutionRoute  String
  urlQuestion    String
  prompt         String
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
